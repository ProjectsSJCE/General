pthread_t threads[num_threads];
offset = (int)(NUMclient / num_threads);
void *status;
end = 0;
for (iter=0; iter<num_threads; iter++)
{
    start = end;
    end += offset;
    if( iter == (num_threads-1))
        end = NUMclient - 1;
    rc = pthread_create(&threads[iter], NULL, function, (void *)arguments(which include start and end)); should void* be there??
}
for (iter=0; iter<num_threads; iter++)
{
    for( iter=0; i < NUM_THREADS; iter++ ){
      rc = pthread_join(threads[iter], &status);
}      
wait for all threads to finish.

